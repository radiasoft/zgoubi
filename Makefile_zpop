FC=gfortran
FFLAGS=-O4 -Wall -fno-automatic -pedantic
CFLAGS=-g -DGFORTRAN4
#FC=ifort
#FFLAGS=-save -xHOST -O3 -ip -no-prec-dir -static -save -shared-intel

.POSIX:

all :
	cd common ; rm libzg.a ; make CFLAGS="$(CFLAGS)" FC="$(FC)" FFLAGS="$(FFLAGS)"
	cd zpop ; make CFLAGS="$(CFLAGS)" FC="$(FC)" FFLAGS="$(FFLAGS)"

